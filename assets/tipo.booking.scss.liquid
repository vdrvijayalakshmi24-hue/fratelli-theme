.confirm{
    height:100%;
    .product-info{
        text-align: center;
        padding : 30px 40px 32px 40px;
        @include respond-to(mobile-portrait){
          padding : 15px;
        }
        .thumb {
            width: 100px;
            height: 100px;
            border-radius: 50%;
            overflow: hidden;
            display: inline-block;
            margin-bottom: 15px;
            box-shadow: 0 10px 50px rgba(37,49,98,0.15);
            img {
              width: 100%;
              height: 100%;
                object-fit: cover;
            }
        }
        .title{
          font-size:24px;
            font-weight:500;
            color: $primaryTextColor;
            @include respond-to(mobile-portrait){
                font-size:18px;
            }
        }
    }
  .booking-info {
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: 20px 40px;
        border-top: 1px solid #E7EAF4;
        border-bottom: .1rem solid #E7EAF4;
        color:$primaryTextColor;
        font-size: 14px;
        font-weight: 400;
        @include respond-to(mobile-portrait){
          padding : 15px;
            flex-direction: column;
            align-items: flex-start;
            > div{
                border-bottom: .1rem solid #E7EAF4;
                width: 100%;
                margin-bottom: 5px;
                padding-bottom: 5px;
                display: flex;
                justify-content: space-between;
                &:last-child{
                  margin-bottom: 0px;
                    padding-bottom: 0px;
                    border-bottom: none;
                }
            }
        }
        label{
          color:$primaryTextColor;
          display: flex;
            align-items: center;
            font-size: 14px;
            font-weight: 500;
            padding-right: 20px;
            svg{
              margin-right:5px;
            }
        }
    }
    .extra-fields{
      padding: 20px 40px;
        display: flex;
        flex-wrap: wrap;
        @include respond-to(mobile-portrait){
          padding : 15px;
        }
        .required-mark{
          color:red;
        }
        .element{
          * {
           color: $primaryTextColor;
          }
          flex: 0 0 50%;
            color:$primaryTextColor;
            border: 10px solid transparent;
            input,textarea,.ss-single-selected{
                width: 100%;
                border-radius: 4px;
                border: 1px solid #E7EAF4;
                color: $primaryTextColor;
                padding: 10px 16px;
                height:unset;
                background: #F8F9FD!important;
            }
            &:nth-child(1),
            &:nth-child(2){
              border-top:none;
            }
            &:nth-child(odd){
              border-left:none;
                border-right-width: 15px;
            }
            &:nth-child(even){
              border-right:none;
                border-left-width: 15px;
            }
            @include respond-to(mobile-portrait){
                flex: 0 0 100%;
                border:none;
                margin-bottom: 10px;
                &:last-child{
                  margin-bottom: 0px;
                }
            }
        }
        .tpb-checkbox{
            display:inline-block;
            @include respond-to(mobile-portrait){
              display:block;
            }
          .ctx {
              -webkit-user-select: none;
              -moz-user-select: none;
                -ms-user-select: none;
                    user-select: none;
              cursor: pointer;
              padding: 6px 8px;
              border-radius: 6px;
              overflow: hidden;
              transition: all 0.2s ease;
              display: inline-block;
              margin-left: -8px;
            }
            .ctx:not(:last-child) {
              margin-right: 8px;
            }
            .ctx:hover {
              background-color: rgba(125, 100, 247, 0.09);
            }
            .ctx span {
              float: left;
              vertical-align: middle;
              -webkit-transform: translate3d(0, 0, 0);
                      transform: translate3d(0, 0, 0);
            }
            .ctx span:first-child {
              position: relative;
              width: 18px;
              height: 18px;
              border-radius: 4px;
              -webkit-transform: scale(1);
                      transform: scale(1);
              border: 2px solid #c8ccd4;
              transition: all 0.2s ease;
            }
            .ctx span:first-child svg {
              position: absolute;
              top: 2px;
              left: 1px;
              fill: none;
              stroke: #fff;
              stroke-width: 2;
              stroke-linecap: round;
              stroke-linejoin: round;
              stroke-dasharray: 16px;
              stroke-dashoffset: 16px;
              transition: all 0.3s ease;
              transition-delay: 0.1s;
              -webkit-transform: translate3d(0, 0, 0);
                      transform: translate3d(0, 0, 0);
            }
            .ctx span:last-child {
              padding-left: 8px;
              line-height: 18px;
            }
            .ctx:hover span:first-child {
              border-color: $primaryTextColor;
            }
            .inp-box:checked + .ctx span:first-child {
              background: $primaryTextColor;
              border-color: $primaryTextColor;
              -webkit-animation: wave 0.4s ease;
                      animation: wave 0.4s ease;
            }
            .inp-box:checked + .ctx span:first-child svg {
              stroke-dashoffset: 0;
            }
        }
        .tpb-radio-group{
            color:$primaryTextColor;
          [type="radio"]:checked,
            [type="radio"]:not(:checked) {
                position: absolute;
                left: -9999px;
            }
            [type="radio"]:checked + label,
            [type="radio"]:not(:checked) + label
            {
                position: relative;
                padding-left: 28px;
                cursor: pointer;
                line-height: 20px;
                display: inline-block;
                color: $primaryTextColor;
            }
            [type="radio"]:checked + label:before,
            [type="radio"]:not(:checked) + label:before {
                content: '';
                position: absolute;
                left: 0;
                top: 0;
                width: 18px;
                height: 18px;
                border: 1px solid #ddd;
                border-radius: 100%;
                background: #fff;
            }
            [type="radio"]:checked + label:after,
            [type="radio"]:not(:checked) + label:after {
                content: '';
                width: 12px;
                height: 12px;
                background: $primaryTextColor;
                position: absolute;
                top: 3px;
                left: 3px;
                border-radius: 100%;
                -webkit-transition: all 0.2s ease;
                transition: all 0.2s ease;
            }
            [type="radio"]:not(:checked) + label:after {
                opacity: 0;
                -webkit-transform: scale(0);
                transform: scale(0);
            }
            [type="radio"]:checked + label:after {
                opacity: 1;
                -webkit-transform: scale(1);
                transform: scale(1);
            }
        }
    }
  .subtotal{
    background:#F8F9FD;
    padding:15px 40px;
    margin-bottom:30px;
