                    background: $time_slot_color_hover;
                  	cursor:pointer;
                }
                &.disabled {
                    color: $day_off_color;
                    text-decoration: line-through;
                }
                &.picked{
                  background:$selected_day_color;
                }
              	&[data-view*="month next"],&[data-view*="month prev"],
                &[data-view*="year next"],&[data-view*="year prev"],
              	&[data-view*="years next"],&[data-view*="years prev"]{
                	font-size:27px;
                }
                &[data-view="month current"]{
                  pointer-events: none;
                }
            }
          	&[data-view=week]>li,
            &[data-view=week]>li:hover{
