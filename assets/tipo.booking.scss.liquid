            }
        }
      	&:nth-child(1) > ul:nth-child(1),
      	&:nth-child(2) > ul:nth-child(1),
        &:nth-child(3) > ul:nth-child(1){
        	display:flex;
            li[data-view="years current"],
          	li[data-view="year current"],
          	li[data-view="month current"]{
            	flex-grow: 1;
              	border-radius:0;
              	font-weight:500;
              	font-size:16px;
            }
        }
    }
