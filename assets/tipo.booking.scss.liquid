.tpb-timepicker {
    display:none;
    flex-wrap:wrap;
    justify-content:space-around;
    max-height: 142px;
    overflow: hidden;
    overflow-y: scroll;
    margin-bottom: 30px;
    &::-webkit-scrollbar {
    	width: 5px;
      	transition:all .3s;
    }

    /* Track */
    &::-webkit-scrollbar-track {
      background:none;
    }

    /* Handle */
    &::-webkit-scrollbar-thumb {
      background: #414C89;
      border-radius: 5px;
    }

    /* Handle on hover */
    &::-webkit-scrollbar-thumb:hover {
      background: #414C89;
    }
    .radiobtn {
        position: relative;
        display: block;
        width: 50%;
        border: 2px solid transparent;
        color: $calender_text_color;
        font-size: 12px;
        font-weight: 300;
      	cursor: pointer;
      	&:nth-child(1),
        &:nth-child(2){
            border-top: none;
        }
        .tooltip{
            width: auto;
            visibility: hidden;
            background-color: $background_color_box_message_time_slot_no_available;
            padding: 5px;
            box-shadow: 0 0 50px 0 rgba(0,0,0,0.3);
            opacity: 0;
            transition: opacity 0.5s ease;
            position: absolute;
            z-index: 20;
            border-radius: 5px;
            color: $text_color_text_box_message_time_slot_no_available;
        }
        &.disabled:hover .tooltip {
            visibility: visible;
            transition: opacity 0.5s ease;
            opacity: 1;
            position: fixed;

        }
        label{
            padding: 7px 10px;
          	width:100%;
            border-radius: 3px;
            text-align: center;
            background: $time_slot_color;
            margin: 0;
            color: $calender_text_color;
            display: block;
            font-size: 12px;
            font-weight: 300;
            text-transform: none;
            &:hover{
            	background:$time_slot_color_hover;
            }
            &.disabled {
                background: $time_slot_no_available_color;
                pointer-events: none;

            }
        }
        input[type="radio"],input[type="checkbox"] {
            display: none;
            position: absolute;
            width: 100%;
            appearance: none;
            &:checked + label {
                background: $time_slot_color_selected;
                &:after {
                    background: #ddd;
                }
                &:before {
                    width: 20px;
                    height: 20px;
                }
            }
        }
    }
}
.action{
  text-align:center;
    margin-bottom: 15px;
  &.fullwidth{
    padding:15px;
  }
    @include respond-to(mobile-portrait){
        padding: 15px;
    }
    button[disabled] {
        cursor: no-drop !important;
        opacity: 0.6;
    }
  	.continue-button {
          width: 100%;
          border: none;
          white-space: nowrap;
          cursor: pointer;
          -webkit-appearance: none;
          text-align: center;
          -webkit-box-sizing: border-box;
          box-sizing: border-box;
          outline: 0;
          -webkit-transition: .1s;
          transition: .1s;
          font-weight: 500;
          -ms-user-select: none;
          padding: 12px 20px;
          font-size: 14px;
          border-radius: 4px;
          background-color: $primaryButtonColor;
          color: #ffffff;
          margin: 0 auto;
          line-height: initial;
    }
    .back-button{
    	background:#F8F9FD;
      	padding:12px 37px;
      	border:1px solid #E7EAF4;
      	border-radius:4px;
      	color:$primaryTextColor;
        margin-bottom:30px;
        line-height: initial;
        &.c2{
			display:none;
        }
        @include respond-to(mobile-portrait){
            width:100%;
            margin-bottom: 15px;
          	&.c2{
                display:block;
            }
        }

    }
    .confirm-button{
    	background:$primaryButtonColor;
      	border-radius:4px;
      	padding:12px 37px;
      	border:1px solid transparent;
      	color:#FFF;
        margin-bottom:30px;
        line-height: initial;
        position:relative;
        @include respond-to(mobile-portrait){
            width:100%;
            margin-bottom: 0;
        }
        &.loading .spinner {
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
            left: 0;
            background: $primaryButtonColor;
            display: -webkit-box;
            display: flex;
            -webkit-box-align: center;
            align-items: center;
            -webkit-box-pack: center;
            justify-content: center;
            &:after {
              content: "";
              border-left: 3px solid transparent;
              border-top: 3px solid transparent;
              border-color: #fff transparent transparent #fff;
              border-style: solid;
              border-width: 3px;
              border-radius: 50%;
              width: 16px;
              height: 16px;
              -webkit-animation: spinButton 1s cubic-bezier(.34,.11,.63,.86) infinite;
              animation: spinButton 1s cubic-bezier(.34,.11,.63,.86) infinite;
          }
        }
    }
}
@keyframes  spinButton {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}
@-webkit-keyframes wave {
  50% {
    -webkit-transform: scale(0.8);
            transform: scale(0.8);
  }
}
@keyframes  wave {
  50% {
    -webkit-transform: scale(0.8);
            transform: scale(0.8);
  }
}


