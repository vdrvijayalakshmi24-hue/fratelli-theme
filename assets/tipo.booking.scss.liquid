.tpb-booking-form {
  	display: flex;
    align-items: center;
    justify-content: center;
    max-width: 840px;
    margin: 48px auto;
    border: 1px solid #e2e6ec;
    border-radius: 10px;
    -moz-box-shadow: 0 10px 50px rgba(37,49,98,0.15);
    -webkit-box-shadow: 0 10px 50px rgba(37,49,98,0.15);
    box-shadow: 0 10px 50px rgba(37,49,98,0.15);
    position: relative;
    background: #fff;
  	@include respond-to(handhelds){
      margin:20px 55px;
    }
  	@include respond-to(mobile-portrait){
      margin:20px;
    }
    & * {
    	font-family : $font;
        box-sizing: border-box;
    }
    .tpb-box-wrapper{
        width: 100%;
        height: 100%;
    }
    .tpb-box{
    	width: 100%;
        background-color: #fff;
        overflow: hidden;
        border-radius: 10px;
      	h2 {
            color: #323a45;
            font-size: 27px;
            line-height: 60px;
            font-style: normal;
            font-weight: 400;
            letter-spacing: 0px;
            text-transform: none;
            text-align: center;
        }
        .content{
            padding: 0;
        	width: 300%;
          	transition: all .5s;
            ul li:before {
              content: unset;
            }
          	.step1{
              	width: 33.3333333%;
              	float:left;
                transition: all 0.5s;
              	background:#fff;
              	padding: 65px 40px 65px 30px;
                background-image: url($background_image);
                background-repeat: no-repeat;
                @include respond-to(mobile-portrait){
                  padding:25px;
                }
              	.inner-step{
                  	width:50%;
                  	margin:0 auto;
                  	transition:all .5s;
                    float:right;
                  	@include respond-to(mobile-portrait){
                      width:100%;
                    }
                  	h5{
                      font-size: 24px;
                      color: $primaryTextColor;
                      font-weight: 400;
                	}
                }
            }
            .step2{
				        width: 33.3333333%;
              	float:left;
                transition: all 0.5s;
                background: $calender_background_color;
              	padding: 65px 50px 65px 50px;
                z-index:9;
                @include respond-to(mobile-portrait){
                	padding: 25px;
                }
            }
            .step3{
            	width: 33.333333%;
              	float:left;
                transition: all 0.5s;
              	background:#fff;
            }
            &[data-step="1"]{
                .step1{}
                .step2{height:0;}
              	.step3{height:0;}
            }
          	&[data-step="2"]{
              	width:200%;
                .step1{
                  width:25%;
                  padding-bottom: calc( 500em + 30px );
    			        margin-bottom: -500em;
                  background-image: unset;
                  .inner-step{
                  	width:100%;
                  }
                }
                .step2{
                  width:25%;
                  padding-bottom: calc( 500em + 30px );
                  margin-bottom: -500em;
                }
                .step3{width:50%;height:0;}
                @include respond-to(mobile-portrait){
                	width:300%;
                  	transform:translate(-33.3333%);
                    .step1{width:33.3333%;height:0;}
                    .step2{width:33.3333%;}
                    .step3{width:33.3333%;height:0;}
                }
            }
          	&[data-step="3"]{
              	width:200%;
              	transform:translate(-50%);
                .step1{width:25%;height:0;}
                .step2{width:25%;height:0;}
                .step3{width:50%;}
                @include respond-to(mobile-portrait){
                	width:300%;
                    transform:translate(-66.6667%);
                    .step1{width:33.3333%;height:0;}
                    .step2{width:33.3333%;height:0;}
                    .step3{width:33.3333%;}
                }
            }
        }

    }
    .timezone-note {
        display:none;
        color: #FFF;
        font-size: 12px;
        justify-content: flex-end;
        align-items: center;
        padding: 0 0 10px;
        .timezone-icon {
            display: inherit;
            margin-right: 5px;
            svg{
              width: 16px;
              height: 16px;
            }
        }
        .currentTime::before {content: ' (';}
        .currentTime::after {content: ') ';}
    }

