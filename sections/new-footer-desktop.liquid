<style>
    .new-footer-desktop {
        padding: 20px;
        background: #10151A;
        color: #8B8B8B;
        text-align: center;
        display: flex;
        flex-direction: column;
        padding: 20px;
        gap: 20px;
    }
    .section-container {
        display: flex;
        gap: 30px;
    }
    .left-section {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        gap: 10px;
    }
    .right-section {
        display: flex;
        gap: 20px;
    }
    .right-section-content {
        display: flex;
        flex-direction: column;
        justify-content: flex-start;
        gap: 10px;
    }
    .right-section-content-title {
        font-size: 14px;
        font-weight: 400;
        text-transform: uppercase;
        text-align: left;
        color: #8B8B8B;
        margin-bottom: 0;
    }
    .right-section-content-list {
        display: flex;
        flex-direction: column;
        justify-content: flex-start;
        list-style: none;
    }
    .right-section-content-list-item {
        font-size: 14px;
        font-weight: 400;
        text-align: left;
        text-transform: uppercase;
        color: #FFFFFF;
    }
    .bottom-section {
        display: flex;
        justify-content: space-between;
    }
    input {
        background: #fff !important;
    }
    .right-section-content p {
        text-align: left;
    }
</style>
